                                                        <%= data2[i].cource_name%>
                                                    </option>
                                                    <% }%>

                                        <% }%>

                                    </select>
                                    <span class="span1" id="city-error"></span>
                                </div>
                            </div>


                                <div class="input-group">
                                    <label for="University">School/college</label>
                                    <input type="text" name="uni" id="uni"
                                        value="<%= result2[j].board_university_name%>" onchange="uniValidate()" />
                                    <span class="span1" id="uni-error"></span>
                                </div>
                                <div class="input-group">
                                    <label for="passyear">Passing Year</label>
                                    <input type="text" name="passyear" id="passyear"
                                        value="<%= result2[j].passout_year%>" onchange="yearValidate()" />
                                    <span class="span1" id="year-error"></span>
                                </div>
                                <div class="input-group">
                                    <label for="percentage">Percentage</label>
                                    <input type="text" name="percent" id="percent" value="<%= result2[j].percentage%>"
                                        onchange="percentValidate()" />
                                    <span class="span1" id="percent-error"></span>
                                </div>


                        </div>
                        <% }%>

                        <div id="add">

                        </div>
                            <div class="btn-grup-2">
                                <p class="btn-2" onclick="education()">Add Field</p>
                                
                            </div>
                            <p id="p1"></p>
                            <div class="btns-group">
                                <p class="btn btn-prev">Previous</p>
                                <p class="btn btn2-next" onclick="return registerPage2()">Next</p>
                              
                            </div>
            </div>
        </div>


        <div class="form-step">
      
            <div class="input-group">
              <label for="ID"> Skills</label>
              <input type="text" name="ID" id="ID" />
            </div>
      
            <div class="btns-group">
              <p class="btn btn-prev">Previous</p>
              <p class="btn btn-next">Next</p>
              
