
    <form action="/wizard_edit" method="post" class="form" enctype="multipart/form-data">
        <div class="container">
            <h1 class="text-center">Registeration Form</h1>
            <!-- Progress bar -->
            <div class="progressbar">
                <div class="progress" id="progress"></div>

                <div class="progress-step progress-step-active" data-title="Basic info"></div>
                <div class="progress-step" data-title="Education Detail"></div>
                <div class="progress-step" data-title="Skills"></div>
                <div class="progress-step" data-title="References"></div>
                <div class="progress-step" data-title="Documents upload"></div>
            </div>

            <!-- Steps -->
            <div class="form-step form-step-active">

                <div class="row">
                    <div class="col">
                        <div class="input-group-3">
                            <label for="username">First Name</label>
                            <input type="text" name="fname" id="fname" value="<%= result1[0].firstname%>"
                                onchange="fnameValidate()" />
                            <span class="span1" id="fname-error"></span>
                        </div>
                    </div>
                    <div class="col">
                        <div class="input-group-3">
                            <label for="position">Last Name</label>

                            <!-- onchange="lnameValidate()" -->
                            <input type="text" name="lname" id="lname" value="<%= result1[0].lastname%>" />
                            <span class="span1" id="lname-error"></span>
                        </div>
                    </div>
                </div>

                <div class="input-group">
                    <label for="dob">Date of Birth</label>
                    <input type="date" name="dob_1" id="dob_1" value="<%= result1[0].birth_date%>" />
                    <span class="span1" id="dob-error"></span>
                </div>
                <div class="input-group">
                    <label for="address">Address</label>
                    <input type="text" name="address" id="address" onchange="addressValidate()"
                        value="<%= result1[0].address%>" />
                    <span class="span1" id="address-error"></span>
                </div>
                <label for="gender">Gender</label>
                <div class="input-group-2">
                    <div class="div-2">
                        <% var arr=['male','female']%>
                            <% for(let i=0;i<arr.length;i++){%>
                                <% if(arr[i]==result1[0].gender){%>
                                    <div> <input type="radio" name="gender" id="male" value="<%= arr[i]%>" checked />
                                        <%= arr[i]%>
                                    </div>

                                    <% }else{%>

                                        <div><input type="radio" name="gender" id="female" value="<%= arr[i]%>" />
                                            <%= arr[i]%>
                                        </div>
                                        <% }%>
                                            <% }%>



                    </div>
                </div>
                <span class="span1" id="gender-error"></span>
                <div class="input-group">
                    <label for="position">Contact</label>
                    <input type="text" name="contact" id="contact" value="<%= result1[0].phone_number%>"
                        onchange="contactValidate()" />
                    <span class="span1" id="contact-error"></span>
                </div>
                <div class="input-group">
                    <label for="position">Email</label>
                    <input type="email" name="email" id="email" value="<%= result1[0].email%>"/>
                    <span class="span1" id="email-error"></span>
                  </div>
                  <div class="input-group">
                    <label for="position">Designation</label>
                    <input type="text" name="designation" id="designation" value="<%= result1[0].designation%>" />
                    <span class="span1" id="designation-error"></span>
                  </div>
                  <div class="input-group">
                    <label for="position">Department</label>
                    <input type="text" name="department" id="department" value="<%= result1[0].department%>"/>
                    <span class="span1" id="department-error"></span>
                  </div>
                <div class="input-group">
                    <label for="position">Relationship Status</label>
                    <div class="select-dropdown">
                        <% var rel=['Single','Married','Divorce']%>

                            <select name="relationship" id="relationship">
                                <% for(let i=0;i<rel.length;i++){%>
                                    <% if(rel[i]==result1[0].relationship){%>

                                        <option value="<%= rel[i]%>" selected="selected">
                                            <%= rel[i]%>
                                        </option>

                                        <% }else{%>

                                            <option value="<%= rel[i]%>">
                                                <%= rel[i]%>
                                            </option>
                                            <% }%>
                                                <% }%>

                            </select>
                            <span class="span1" id="relationship-error"></span>
                    </div>

                </div>
                <div class="row">


                    <div class="col">
                        <div class="input-group-3">
                            <label for="position"> State</label>
                            <div class="select-dropdown">
                                <select name="state" id="state" onclick="select(this)">
                                    <option value="" selected disabled> select option</option>
                                    <% for(let i=0;i<data3.length ;i++) {%>
                                        <% if(result1[0].state==data3[i].state_name){%>

                                            <option value="<%= data3[i].state_name%>" selected>
                                                <%= data3[i].state_name%>
                                            </option>

                                            <% }else{%>

                                                <option value="<%= data3[i].state_name%>">
                                                    <%= data3[i].state_name%>
                                                </option>
                                                <% }%>

                                                    <% }%>
                                </select>
                            </div>
                            <span class="span1" id="state-error"></span>
                        </div>
                    </div>





                    
                    <div class="col">
                        <div class="input-group-3">
                            <label for="position"> City</label>
                            <div class="select-dropdown">
                                <select name="city" id="city">
                                    <option value="<%= result1[0].city%>">
                                        <%= result1[0].city%>
                                    </option>
                                </select>
                            </div>
                            <span class="span1" id="city-error"></span>
                        </div>
                    </div>
                </div>
                <div class="">
                    <p class="btn btn-next width-50 ml-auto" onclick="return registerPage1()">
                        Next
                    </p>
                    
                </div>
            </div>

        </div>

        <div class="container">
            <div class="form-step">
