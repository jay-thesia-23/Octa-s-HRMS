 <section class="home">
        <div class="ag-format-container">

            <div class="online_card mb_50">

                <div class="attendence_crd mrg_10 box_shadow_2">
                    <div class="attendence_crd_stl" id="Attendancecrd1">
                        
                          <div>
                            <div class="online_txt">Attendance</div>
                            <div class="online_count">145</div>
                          </div>
                    </div>
                </div>
        
                <div class="attendence_crd mrg_10 box_shadow_2">
                    <div class="attendence_crd_stl" id="Attendancecrd2">
                        
                          <div>
                            <div class="online_txt">Late Days</div>
                            <div class="online_count">145</div>
                          </div>
                    </div>
                </div>
        
                <div class="attendence_crd mrg_10 box_shadow_2">
                    <div class="attendence_crd_stl" id="Attendancecrd3">
                        
                          <div>
                            <div class="online_txt">Absent</div>
                            <div class="online_count">145</div>
                          </div>
                    </div>
                </div>
        
                <div class="attendence_crd mrg_10 box_shadow_2">
                    <div class="attendence_crd_stl" id="Attendancecrd4">
                        
                          <div>
                            <div class="online_txt">Half Days</div>
                            <div class="online_count">145</div>
                          </div>
                    </div>
                </div>
              </div>
        
              <div>
                <table>
                  <thead>
                <tr>
                    <th>Date</th>
                    <th>Entry time</th>
                    <th>Exit time</th>
                    <th>Break time</th>
                    <th>Working hours</th>
                    <th>Other Details</th>
                </tr>
            </thead>
            <tbody>

                <% for(let i=0; i<startdate.length; i++) {%>
                    <tr class="clr_blue">
                        <td>
                            <%=startdate[i]%>
                        </td>

                        <td>
                            <%=starttime[i]%>
                        </td>

                        <td>
                            <%=exittime[i]%>
                        </td>

                        <td>1:00</td>

                        <td>
                            <progress value="<%=progress[i]%>" max="100"> </progress>
                        </td>
                        <td></td>

                    </tr>

                    <% } %>
            </tbody>

              
                </table>
              </div>

            <div class="part4">
                <div class="row-page">
                    <div class="m_t_b">
                        <p>Show all entries</p>
                    </div>
                    <div>
        
        
                        <% for(let i=0; i<pagearray; i++) { %>
                            <% if (i==(pid-1)) { %>
                               <a id="pageno" href="/attendance?pid=<%= (i+1) %>"><%= (i+1) %></a>
                                <%} else { %>
                                <a  id="pageno" href="/attendance?pid=<%= (i+1) %>"><%= (i+1) %></a>
                                <% } %>  
                                <% } %> 
                    </div>
        
                </div>
            </div>
        </div>
        
        
    </section>
    <script>

        var mode_switch = document.getElementById('mode_btn')
        var Attendancecrd1 = document.getElementById('Attendancecrd1')
        var Attendancecrd2 = document.getElementById('Attendancecrd2')
        var Attendancecrd3 = document.getElementById('Attendancecrd3')
        var Attendancecrd4 = document.getElementById('Attendancecrd4')
  
        
        mode_switch.addEventListener('click',function(){
          Attendancecrd1.classList.toggle('dark_mode')
          Attendancecrd2.classList.toggle('dark_mode')
          Attendancecrd3.classList.toggle('dark_mode')
          Attendancecrd4.classList.toggle('dark_mode')
        })
    </script>
