    <div class="calender-main">
        <header>
            <p class="current-date"></p>
            <p class="hidden-month" hidden></p>
            <div class="icons">
            <span onclick="monthChange(-1);" id="prev"><svg viewBox="-2 -2 24 24" width="40" height="40" fill="none" xmlns="http://www.w3.org/2000/svg"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <rect width="24" height="24" fill="white"></rect> <path fill-rule="evenodd" clip-rule="evenodd" d="M2 12C2 6.47715 6.47715 2 12 2C17.5228 2 22 6.47715 22 12C22 17.5228 17.5228 22 12 22C6.47715 22 2 17.5228 2 12ZM13.7071 8.29289C14.0976 8.68342 14.0976 9.31658 13.7071 9.70711L11.4142 12L13.7071 14.2929C14.0976 14.6834 14.0976 15.3166 13.7071 15.7071C13.3166 16.0976 12.6834 16.0976 12.2929 15.7071L9.5554 12.9696C9.0199 12.4341 9.0199 11.5659 9.55541 11.0304L12.2929 8.29289C12.6834 7.90237 13.3166 7.90237 13.7071 8.29289Z" fill="#919191"></path> </g></svg></span>
            <span onclick="monthChange(+1);" id="next"><svg viewBox="-2 -2 24 24" width="40" height="40" fill="none" xmlns="http://www.w3.org/2000/svg"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <rect width="24" height="24" fill="white"></rect> <path fill-rule="evenodd" clip-rule="evenodd" d="M2 12C2 6.47715 6.47715 2 12 2C17.5228 2 22 6.47715 22 12C22 17.5228 17.5228 22 12 22C6.47715 22 2 17.5228 2 12ZM10.2929 15.7071C9.90237 15.3166 9.90237 14.6834 10.2929 14.2929L12.5858 12L10.2929 9.70711C9.90237 9.31658 9.90237 8.68342 10.2929 8.29289C10.6834 7.90237 11.3166 7.90237 11.7071 8.29289L14.4229 11.0087C14.9704 11.5562 14.9704 12.4438 14.4229 12.9913L11.7071 15.7071C11.3166 16.0976 10.6834 16.0976 10.2929 15.7071Z" fill="#919191"></path> </g></svg></span>
            </div>
        </header>
        <table class="calendar">
            <tr class="weeks">
                <th>Sun</th>
                <th>Mon</th>
                <th>Tue</th>
                <th>Wed</th>
                <th>Thu</th>
                <th>Fri</th>
                <th>Sat</th>
            </tr>
        </div>
    </div>
    <script>
        let date = new Date();
        let monthArr =  ["","January","February","March","April","May","June","July","August","September","October","November","December"];
        let monthInNum = document.querySelector('.hidden-month');

        let year = date.getFullYear();
        let month = date.getMonth();
        createCalendarTable();
        let d = new Date(year, month);
        let start = d.getDay();
        dataInCalendar(month+1, year, start+1);
        initalizAll(false);

        function getDaysInMonth(month,year) {
            return new Date(year, month, 0).getDate();
        }
        function createCalendarTable(){
            monthInNum.innerHTML = month+1;
            let tbl = document.querySelector('.calendar');
            let count = 1;
            for(let row = 1; row < 7; row++){
                let tr = tbl.insertRow(row);
                for(let cell = 0; cell < 7; cell++){
                    let td =  tr.insertCell(cell);
                    td.className = 'td'+count;
                    tr.appendChild(td);
                    count ++;
                }   
                tbl.appendChild(tr);
            }
        }

        function dataInCalendar(month,year,start){
            let max = getDaysInMonth(month,year);   
            let count = 1;
            for(let tempDate = 1; tempDate < 42; tempDate++){
                let Creclass = 'td'+ tempDate;
                let ptag = document.querySelector(`.${Creclass}`);      
                ptag.innerHTML = "";
                if((tempDate >= start) && tempDate < max+start){    
                    p = document.createElement('p');
                    p.id = count;
                    p.style.marginTop = '10px';
                    ptag.appendChild(p);
                    p.innerHTML = count;
                    count ++;
                }
            }
            count = 1;
        }
        function initalizAll(isMonthChange){

            let today = date.getDate();
            let todayStyle = document.getElementById(today);
            if(isMonthChange){
                todayStyle.style.background = 'none';
                todayStyle.style.color = 'black';
            }
            else{
                todayStyle.style.backgroundColor = 'rgb(0,164,0)';
                todayStyle.style.color = 'white';
                todayStyle.style.borderRadius = '100%';
                todayStyle.style.padding = '10px';  
            }
            let  MonthDetail =monthArr[month+1] +" "+ year;
            document.querySelector('.current-date').innerHTML =  MonthDetail;
        }
        function monthChange(newData){
            let newMonth = Number(monthInNum.innerHTML) + newData;
            let newYear = year;
            if(newMonth == 0){
                newMonth = 12;
                newYear = year-1;
            }
            else if(newMonth == 13){
                newMonth = 1;
                newYear = year+1;
            }
            monthInNum.innerHTML = newMonth;
            
            let d = new Date(year, (newMonth-1));
            start = d.getDay();
            dataInCalendar(newMonth, year, start+1);
            
            let  MonthDetail =monthArr[newMonth] +" "+ year;
            document.querySelector('.current-date').innerHTML =  MonthDetail;
            if((newMonth == month+1) && (newYear == year)){
                initalizAll(false);
            }

            }
    </script>